@model PaginaPessoal2.Models.Habilitacoes

@{
    ViewData["Title"] = "Details";
}

<header class="masthead text-center p-5">
    <div class="masthead-content">
        <div class="container">
            <h1 class="display-4 p-5 mb-0">Detalhes</h1>

        </div>
    </div>

</header>

<section>
    <div class="container p-5">

        <article>
            <div>
                <h4>Habilitacoes</h4>
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Ano)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Ano)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Curso)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Curso)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Instituicao)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Instituicao)
                    </dd>
                </dl>
            </div>
            <div>
                <a class="btn btn-warning" asp-action="Edit" asp-route-id="@Model.HabilitacoesId">Editar</a> |
                <a class="btn btn-secondary" asp-action="Index">Voltar à lista</a>
            </div>


        </article>

    </div>

</section>